{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MDWDataFactory-53a5g"
		},
		"MDWCosmosDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MDWCosmosDB'"
		},
		"MDWDataLake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MDWDataLake'"
		},
		"MDWDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'MDWDatabricks'"
		},
		"MDWSQLServer_NYCDataSets_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MDWSQLServer_NYCDataSets'"
		},
		"MDWSQLServer_NYCDataSets_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MDWSQLServer_NYCDataSets'"
		},
		"MDWSQLVirtualServer_MDWASQLDW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MDWSQLVirtualServer_MDWASQLDW'"
		},
		"MDWResources_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'MDWResources'"
		},
		"MDWSQLServer_NYCDataSets_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "MDWAdmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MDWCosmosDB_connectionString": {
						"value": "[parameters('MDWCosmosDB_connectionString')]"
					},
					"MDWDataLake_connectionString": {
						"value": "[parameters('MDWDataLake_connectionString')]"
					},
					"MDWDatabricks_accessToken": {
						"value": "[parameters('MDWDatabricks_accessToken')]"
					},
					"MDWSQLVirtualServer_MDWASQLDW_connectionString": {
						"value": "[parameters('MDWSQLVirtualServer_MDWASQLDW_connectionString')]"
					},
					"MDWSQLServer_NYCDataSets_connectionString": {
						"value": "[parameters('MDWSQLServer_NYCDataSets_connectionString')]"
					},
					"MDWSQLServer_NYCDataSets_password": {
						"value": "[parameters('MDWSQLServer_NYCDataSets_password')]"
					},
					"MDWResources_sasUri": {
						"value": "[parameters('MDWResources_sasUri')]"
					},
					"MDWSQLServer_NYCDataSets_properties_typeProperties_userName": {
						"value": "[parameters('MDWSQLServer_NYCDataSets_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}